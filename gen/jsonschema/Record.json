    "$ref": "#/definitions/Record",
    "definitions": {
        "Record": {
            "properties": {
                "title": {
                    "type": "string",
                    "description": "Core metadata"
                },
                "alt_title": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "abstract": {
                    "type": "string"
                },
                "contributors": {
                    "items": {
                        "$ref": "#/definitions/hub.v1.Contributor"
                    },
