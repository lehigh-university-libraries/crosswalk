                },
                "table_of_contents": {
                    "type": "string"
                },
                "source": {
                    "type": "string"
                },
                "digital_origin": {
                    "type": "string"
                },
                "edition": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                },
