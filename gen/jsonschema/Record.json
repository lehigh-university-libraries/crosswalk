                    "type": "string"
                },
                "object_model": {
                    "type": "string",
                    "description": "Technical Metadata"
                },
                "add_coverpage": {
                    "type": "boolean"
                },
                "capture_device": {
                    "type": "string"
                },
                "ppi": {
                    "type": "integer"
                },
                "page_count": {
                    "type": "integer",
                    "description": "Physical description details"
                },
                "dimensions": {
                    "type": "string"
                },
                "duration": {
                    "type": "string"
                },
                "archival_location": {
                    "$ref": "#/definitions/hub.v1.ArchivalLocation",
                    "additionalProperties": true,
                    "description": "Archival info"
                },
                "files": {
                    "items": {
                        "$ref": "#/definitions/hub.v1.File"
                    },
                    "type": "array",
                    "description": "Files"
                },
                "physical_form": {
                    "items": {
                        "$ref": "#/definitions/hub.v1.Subject"
                    },
                    "type": "array",
                    "description": "Physical form (e.g. Getty AAT)"
                },
                "relations": {
                    "items": {
                        "$ref": "#/definitions/hub.v1.Relation"
                    },
                    "type": "array",
                    "description": "Relations to other works"
                },
                "degree_info": {
                    "$ref": "#/definitions/hub.v1.DegreeInfo",
                    "additionalProperties": true,
                    "description": "Thesis/dissertation specific"
                },
                "funders": {
                    "items": {
                        "$ref": "#/definitions/hub.v1.Funder"
                    },
                    "type": "array",
                    "description": "Funding information"
                },
                "full_title": {
                    "type": "string",
                    "description": "Full title (when different from display title)"
                },
                "departments": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array",
                    "description": "Related departments/units (for non-thesis items like faculty publications)"
                },
                "local_restriction": {
                    "type": "string",
                    "description": "Local access restrictions beyond rights statements"
                },
                "geographic": {
                    "$ref": "#/definitions/hub.v1.HierarchicalGeographic",
                    "additionalProperties": true,
                    "description": "Structured geographic location"
                },
                "extra": {
                    "additionalProperties": true,
                    "type": "object",
                    "description": "Extra holds additional fields that don't map to standard Hub fields. Used for round-trip preservation and format-specific data. 1. This is a TEMPORARY home - if a field appears in \u003e50% of records,    promote it to a first-class field in the Hub schema. 2. Enforce consistent types - don't let spokes mix string \"10\" and int 10. 3. Use machine names as keys (dc_creator), never human labels (\"Creator\"). 4. Data here is NOT indexed/searchable - only use for pass-through. 5. Store semantics, not presentation (ISO dates, not \"March 15, 2024\"). Periodic audit: Run `crosswalk audit-extras` to find promotion candidates."
                },
                "source_info": {
                    "$ref": "#/definitions/hub.v1.SourceInfo",
                    "additionalProperties": true,
                    "description": "Source metadata - tracks where this record came from for auditing."
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Record",
            "description": "Record represents a single scholarly work with its metadata. This is the central type of the hub - all formats convert to/from this."
        },
