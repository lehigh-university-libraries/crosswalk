                        "$ref": "#/definitions/hub.v1.Rights"
                    },
                    "type": "array",
                    "description": "Rights and access"
                },
                "is_public": {
                    "type": "boolean"
                },
                "access_condition": {
                    "type": "string"
                },
                "identifiers": {
                    "items": {
                        "$ref": "#/definitions/hub.v1.Identifier"
                    },
